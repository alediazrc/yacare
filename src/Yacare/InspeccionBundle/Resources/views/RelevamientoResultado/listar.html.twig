{% extends 'TapirBaseBundle::layout.html.twig' %}

{% block pagina_titulo %}Relevamientos - Tipos de resultado{% endblock %}

{% block pagina_contenido %}
<table class="table table-hover">
    <thead>
        <tr>
            <th> </th>
            <th>Grupo</th>
            <th>Nombre</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a data-toggle="ajax-link" class="btn btn-default btn-xs hidden-print" href="{{ path('yacare_inspeccion_relevamientoresultado_editar', { 'id': entity.Id }) }}"><i class="fa fa-edit"></i> Editar</a></td>
            <td>{{ entity.Grupo }}</td>
            <td>{{ entity.nombre }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>

{% include 'TapirBaseBundle::paginar.html.twig' %}

{% endblock %}

{% block pagina_acciones2 %}
<a data-toggle="ajax-link" href="{{ path('yacare_inspeccion_relevamientoresultado_editar_1') }}" class="btn btn-primary"><i class="fa fa-plus"></i> Nuevo tipo de resultado</a>
{% endblock %}
