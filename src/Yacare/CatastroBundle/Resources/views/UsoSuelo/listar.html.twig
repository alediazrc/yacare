{% extends 'TapirBaseBundle:Default:listar.html.twig' %} {% block
pagina_titulo %}CPU / Uso del suelo{% endblock %} {% block
pagina_contenido %} {% include 'TapirBaseBundle::buscar.html.twig' %}

<table class="table table-hover">
	<thead>
		<tr>
			<th></th>
			<th>Código</th>
			<th>Categoría</th>
			<th>Seccion</th>
			<th>Nombre</th>
		</tr>
	</thead>
	<tbody>
		{% for entity in entities %}
		<tr>
			<td><a data-toggle="ajax-link"
				class="btn btn-default btn-xs hidden-print showonhover"
				href="{{ path(baseroute ~ '_editar', { 'id': entity.id }) }}"><i
					class="fa fa-edit"></i> Editar</a></td>
			<td>{{ entity.Codigo }}</td>
			<td>{{ entity.Categoria }}</td>
			<td>{{ entity.Seccion }}</td>
			<td>{{ entity.Nombre }}</td>
		</tr>
		{% endfor %}
	</tbody>
</table>

{% include 'TapirBaseBundle::paginar.html.twig' %} {% endblock %}
